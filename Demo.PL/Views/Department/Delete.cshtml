@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1 class="text-danger">Are You Sure Want to Delete this Department ???? </h1>
<br />
<br />

<form asp-action="Delete">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label class="col col-form-label" asp-for="Code"></label>
        <input class="form-control" asp-for="Code" readonly />
    </div>

    <div class="form-group">
        <label class="col col-form-label" asp-for="Name"></label>
        <input class="form-control" asp-for="Name" readonly />
    </div>

    <div class="form-group">
        <label class="col col-form-label" asp-for="DateOfCreation"></label>
        <input class="form-control" asp-for="DateOfCreation" readonly />
    </div>

    <input type="submit" value="Delete" class="btn btn-danger" /> | 
    <a asp-action="Index">Return To Departments</a>
</form> 


@section ValidationScriptSection {
    <partial name="_ValidationScriptsPartial" />
}
