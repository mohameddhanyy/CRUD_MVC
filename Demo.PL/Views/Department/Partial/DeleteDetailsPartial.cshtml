@model Department
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div asp-validation-summary="All"></div>
<div class="form-group">
    <label class="col col-form-label" asp-for="Code"></label>
    <input class="form-control" asp-for="Code" readonly />
</div>

<div class="form-group">
    <label class="col col-form-label" asp-for="Name"></label>
    <input class="form-control" asp-for="Name" readonly />
</div>

<div class="form-group">
    <label class="col col-form-label" asp-for="DateOfCreation"></label>
    <input class="form-control" asp-for="DateOfCreation" readonly />
</div>
